read
store [-1] //n
store [-2] //i

readloop:
	read
	store [[-2]]
	load [-2]
	add -1
	store [-2]
jg readloop

iloop:
	load [-1]
	add -1
	halt
	store [-2]
	
	jloop:
		load [[-2]] 
		neg			
		add [[-1]]	
		jg endif	
			load [[-2]]
			store [-3]
			load [[-1]]
			store [[-2]]
			load [-3]
			store [[-1]]
		endif:
		load [-2]
		add -1
		store [-2]
	jg jloop
	load [[-1]]
	write
	load [-1]
	add -1
	store [-1]
jg iloop
halt